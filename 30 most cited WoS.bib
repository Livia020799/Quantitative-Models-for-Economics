Author = {Wang, Jing and Wang, Jian-qiang and Tian, Zhang-peng and Zhao, Dong-yan},
Title = {A multihesitant fuzzy linguistic multicriteria decision-making approach
   for logistics outsourcing with incomplete weight information},
